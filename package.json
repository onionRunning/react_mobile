{
  "name": "chenyu",
  "sideEffects": [
    "*.css"
  ],
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.16.4",
    "http-proxy-middleware": "^0.19.1",
    "axios": "^0.18.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "yarn fix:lint",
      "git add"
    ],
    "src/**/*.{js,jsx,ts,tsx,json,css}": [
      "yarn fix:prettier",
      "git add"
    ]
  },
  "scripts": {
    "dev": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "test:ci": "CI=true yarn test",
    "test:cov": "CI=true yarn test --coverage",
    "fix": "yarn fix:prettier && yarn fix:lint",
    "fix:prettier": "prettier --single-quote --write 'src/**/*.{js,jsx,ts,tsx,json,css}'",
    "fix:lint": "eslint --fix --ext .jsx,.js,.tsx,.ts ./src",
    "5elint": "5elint"
  },
  "jest": {
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "clover",
      "cobertura"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not ie <= 11",
    "not op_mini all"
  ],
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "printWidth": 120
  },
  "devDependencies": {
    "@craco/craco": "^3.5.0",
    "@types/blueimp-md5": "^2.7.0",
